{
  "name": "qrcode-generator",
  "private": true,
  "version": "0.0.1",
  "author": "Bruce Song <recall4056@gmail.com>",
  "license": "MIT",
  "description": "Generate qrcode by your peference.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recallwei/qrcode-generator.git"
  },
  "keywords": [
    "qrcode",
    "qrcode-generator",
    "vue"
  ],
  "bugs": {
    "url": "https://github.com/recallwei/qrcode-generator/issues"
  },
  "homepage": "https://github.com/recallwesi/qrcode-generator#readme",
  "scripts": {
    "dev": "pnpm --filter=site dev",
    "build": "pnpm --filter=site build",
    "build:fast": "pnpm --filter=site build:fast",
    "build:pkg": "pnpm -r --stream build:pkg",
    "build:pkg:fast": "pnpm -r --stream build:pkg:fast",
    "check:type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "check:spell": "npx cspell '**'",
    "check:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "pnpm": "^7.27.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tsup": "^6.6.0",
    "typescript": "~4.9.5"
  }
}
